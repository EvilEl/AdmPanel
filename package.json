  },
  "dependencies": {
    "@commitlint/types": "^19.5.0",
    "@mdi/font": "^7.4.47",
    "@sentry/vue": "^9.1.0",
    "@vueuse/core": "^12.7.0",
    "node-modules-inspector": "^0.1.9",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuedraggable": "4.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "prettier": "3.5.3",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-plugin-vuetify": "^2.1.0",
    "vitest": "^3.0.6",
    "vue-tsc": "^2.2.0",
