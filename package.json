{
  "name": "vueadmin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "prettier . --write",
    "lint:fix": "pnpm lint --fix",
    "node:inspect": "pnpx node-modules-inspector",
    "prepare:hooks": "simple-git-hooks"
  },
  "dependencies": {
    "@commitlint/types": "^19.8.0",
    "@mdi/font": "^7.4.47",
    "@sentry/vue": "^9.6.0",
    "@vueuse/core": "^13.0.0",
    "node-modules-inspector": "0.5.5",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuedraggable": "4.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.4",
    "jsdom": "^26.0.0",
    "prettier": "3.5.3",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vite-plugin-vuetify": "^2.1.0",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8",
    "vuetify": "^3.7.16"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commitlint --edit"
  }
}
